<hibernate-mapping>
  <class name="cz.zcu.kiv.eegdatabase.data.pojo.ScenarioSchemas" schema="PBORIK" table="SCENARIO_SCHEMA">
    <id name="schemaId" type="int">
      <column name="SCHEMA_ID" precision="22" scale="0"/>
      <generator class="increment"/>
    </id>
    <property name="schemaName" type="string">
      <column name="SCHEMA_NAME"/>
    </property>
    <property name="sql" type="clob">
        <column name="SQL"/>
    </property>
    <property name="hbmXml" type="clob">
        <column name="HBM_XML"/>
    </property>
    <property name="pojo" type="clob">
        <column name="POJO"/>
    </property>
    <property name="bean" type="string">
        <column name="BEAN"/>
    </property>
    <property name="description" type="string">
        <column name="DESCRIPTION"/>
    </property>
    <property name="approved" type="char">
        <column name="APPROVED"/>
    </property>
  </class>
</hibernate-mapping>
